<% headers = [
  'Paid Date',
  'Client',
  'Amount',
  'Payment Type',
  'Description'
] %>
<%= CSV.generate_line headers %>
<% Invoice.paidByYear(@year.year).sort { |a,b| a.paiddate <=> b.paiddate }.each do |invoice| %><%= CSV.generate_line([
    invoice.paiddate,
    invoice.client.name.sub(',', ''),
    invoice.cost,
    invoice.paymenttype,
    invoice.description.sub(',', '')
  ]) %><% end %>
